 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=r0);
    Not(in=b, out=r1);
    Nand(a=r0, b=r1, out=out);
}
