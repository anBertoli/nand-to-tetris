/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    //a
    Not(in=sel, out=a0);
    And(a=a0, b=in, out=a);

    //b
    And(a=sel, b=in, out=b);
}
